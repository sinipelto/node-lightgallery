    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\server.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Current File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${cwd}\\${file}"
        },
		{
            "type": "node",
            "request": "launch",
            "name": "Run thumbnails",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\process_thumbs.js"
        }
    ]
}